{
  "name": "next-codegen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "json-server": "json-server --watch ./fakeapis/index.js --port 8081",
    "development": "next dev",
    "check-types": "tsc --pretty --noEmit",
    "build": "next build",
    "start-server": "next start",
    "dev": "concurrently --kill-others \"npm run development\" \"npm run json-server\"",
    "start": "concurrently --kill-others \"npm run start-server\" \"npm run json-server\"",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "yarn run lint:fix"
 },
 "husky": {
  "hooks": {
    "pre-commit": "lint-staged"
  }
},
  "browser": {
    "fs": false,
    "child_process": false
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.14.17",
    "axios": "^1.0.0",
    "concurrently": "^8.2.2",
    "currency-symbol-map": "^5.1.0",
    "i18next": "^22.4.15",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "next": "13.3.4",
    "next-i18next": "^13.2.2",
    "notistack": "^2.0.5",
    "plaiceholder": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-i18next": "^12.2.2",
    "react-toastify": "^9.1.3",
    "sharp": "^0.32.6",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/node": "18.6.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^7.0.0",
    "prettier": "2.6.2",
    "typescript": "4.7.4"
  }
}
